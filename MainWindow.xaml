<Window x:Class="ProjektAdmin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektAdmin"
        mc:Ignorable="d"
        Title="Admin App" Height="850" Width="1800" Background="White">
    <Grid>
        <TextBox x:Name="OutputTextBox" HorizontalAlignment="Left" Height="534" Margin="10,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="417"/>
        <Button x:Name="btn_prikaz1" Content="Příkaz" HorizontalAlignment="Left" VerticalAlignment="Top" Width="324" Height="108" Margin="1431,682,0,0" Click="Button_Click" FontSize="48"/>
        <TextBox x:Name="tb_prikaz" HorizontalAlignment="Left" Margin="10,23,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="362" Height="97" KeyDown="tb_prikaz_KeyDown"/>
        <Button Content="Vytvořit učet" HorizontalAlignment="Left" Margin="1431,544,0,0" VerticalAlignment="Top" Height="115" Width="324" FontSize="48" Click="Button_Click_1"/>
        <Button Content="Přihlásit" HorizontalAlignment="Left" Margin="1431,392,0,0" VerticalAlignment="Top" Height="115" Width="324" FontSize="48" Click="Button_Click_2"/>
        <Button x:Name="btn_zobrazit" Content="Zobrazit učty" HorizontalAlignment="Left" Margin="1431,241,0,0" VerticalAlignment="Top" Height="115" Width="324" FontSize="48" Click="btn_zobrazit_Click"/>
    </Grid>
</Window>
